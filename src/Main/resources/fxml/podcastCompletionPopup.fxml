<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="218.0" prefWidth="342.0" stylesheets="@../styles/Main%20Style.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Main.PodcastCompletionController">
   <children>
      <ButtonBar layoutX="143.0" layoutY="164.0" prefHeight="40.0" prefWidth="177.0">
        <buttons>
          <Button fx:id="libraryBtn" mnemonicParsing="false" onAction="#addBackToLibrary" text="Library" />
            <Button fx:id="queueBtn" mnemonicParsing="false" onAction="#addToBottomQueue" text="Queue" />
        </buttons>
      </ButtonBar>
      <Label layoutX="20.0" layoutY="92.0" text="Where would you like your completed podcast to go?" />
   </children>
</AnchorPane>
